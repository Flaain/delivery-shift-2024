{
    "tabWidth": 4,
    "printWidth": 120,
    "importOrder": [
        "^react$",
        "<THIRD_PARTY_MODULES>",
        "interface",
        "(?=content|api)",
        "context/",
        "mock/",
        "config",
        "utils/",
        "hooks/",
        "(components/|./index)",
        ".svg",
        "^../(.*)$",
        "(?=./styles.module.scss)"
    ],
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}